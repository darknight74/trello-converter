<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="warn" strict="true">
  <Appenders>
    <Appender type="Console" name="STDOUT">
      <Layout type="PatternLayout" pattern="%d{yyyyMMddHHmmss} - %p - %c - %m%n"/>
    </Appender>
  </Appenders>
  <Loggers>
    <Root level="info">
      <AppenderRef ref="STDOUT"/>
    </Root>
  </Loggers>
 
</Configuration>